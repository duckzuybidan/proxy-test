# .gitignore for Node.js

# Dependency directories
node_modules/
jspm_packages/

# Optional npm cache directory
.npm/

# dotenv environment variables file
.env
.env.test
.env.*.local

# Logs
logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*

# OS generated files
.DS_Store
Thumbs.db

# Build output
dist/
build/
tmp/
temp/
out/

# IDEs and editors
.vscode/
.idea/
*.suo
*.ntvs*
*.njsproj
*.sln

# Coverage directory used by tools like istanbul
coverage/

# Optional lock files
package-lock.json
yarn.lock
pnpm-lock.yaml

# Optional test reports
test-output/
